{
  "name": "insight-back-end",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "npm run build",
    "stop": "egg-scripts stop --title=egg-server-insight",
    "restart": "npm run stop && npm run start",
    "dev": "npm run clean && egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "build": "npm run tsc && egg-scripts start --daemon --workers=1 --title=egg-server-insight"
  },
  "dependencies": {
    "@forsigner/egg-typeorm": "^1.3.2",
    "@vercel/node": "^1.15.3",
    "crypto": "^1.0.1",
    "egg": "^2.22.2",
    "egg-cors": "^2.2.3",
    "egg-healthy": "^1.0.0",
    "egg-scripts": "^2.11.0",
    "egg-view-nunjucks": "^2.2.0",
    "moment": "^2.24.0",
    "mongodb": "^3.4.0",
    "mysql2": "^2.0.2",
    "node-schedule": "^1.3.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.0",
    "egg-mock": "^3.23.1",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=10.14.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
